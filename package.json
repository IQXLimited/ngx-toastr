{
  "name": "ngx-toastr",
  "description": "Toastr for Angular",
  "homepage": "https://ngx-toastr.vercel.app",
  "repository": "scttcper/ngx-toastr",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --configuration production --no-progress",
    "build": "ng-packagr -p src/lib/ng-package.json",
